{
  "spec": {
    "template": {
      "spec": {
        "volumes": [
          {
            "name": "server-js",
            "configMap": {
              "name": "frontend-server-js"
            }
          }
        ],
        "containers": [
          {
            "name": "frontend",
            "image": "billin19/frontend:prod",
            "volumeMounts": [
              {
                "name": "server-js",
                "mountPath": "/app/server.js",
                "subPath": "server.js"
              }
            ]
          }
        ]
      }
    }
  }
}
